
 <div class="container">
  <div class="container header-block">
    <h3><%= gettext "Choose your plan according to your monthly volume" %></h3>
    <p><%= gettext "Each phone number can send messages up to 250 recipients daily. For example, 2 phone number can
      send messages up to 500 recipients daily." %>
    </p>
     <p><%= gettext "( 1 phone number daily limit is 250 messages. )" %></p>
  </div>
   <div class="plan-flexbox">

        <div class="plan-flexitem" id="Free">
          <%= form_for @conn, plan_path(@conn, :create), fn f -> %>
            <div class="card card-pricing card-raised">
              <div class="card-content">
                <h6 class="plan-name"><%= gettext "Free" %></h6>
                <!--
                <div class="icon icon-rose">
                  <i class="material-icons">home</i>
                </div>
              -->
                <h3 class="card-title plan-price">$0</h3>
                <p class="card-description">
                  0 <%= gettext "dedicated business phone number" %>.
                </p>
                <p class="card-description">
                  <%= gettext "Daily limit is" %> 50 <%= gettext "messages"%>.
                </p>
                <%= hidden_input f, :plan_id, class: "plan", value: @free_plan_id  %>
                <%= submit id: "Free-Button", class: "btn btn-github btn-round choose-plan-button" do %>
                  <span class="fas fa-spinner fa-spin fa-lg hide-icon"></span>
                  <%= gettext "Choose Plan" %>
                <% end %>
              </div>
            </div>
          <%end%>
        </div>



        <div class="plan-flexitem" id="Bronze">
          <%= form_for @conn, plan_path(@conn, :create), fn f -> %>
          <div class="card card-pricing card-raised">
            <div class="card-content">
              <h6 class="plan-name"><%= gettext "Bronze" %></h6>
              <!--
              <div class="icon icon-rose">
                <i class="material-icons">home</i>
              </div>
            -->
              <h3 class="card-title plan-price">$5</h3>
              <p class="card-description">
                1 <%= gettext "dedicated business phone number" %>.
              </p>
              <p class="card-description">
                <%= gettext "Daily limit is" %> 250 <%= gettext "messages"%>.
              </p>
              <%= hidden_input f, :plan_id, class: "plan", value: @bronze_plan_id  %>
              <%= submit id: "Bronze-Button", class: "btn btn-github btn-round choose-plan-button" do %>
                <span class="fas fa-spinner fa-spin fa-lg hide-icon"></span>
                <%= gettext "Choose Plan" %>
              <% end %>
            </div>
          </div>
          <% end %>
        </div>



