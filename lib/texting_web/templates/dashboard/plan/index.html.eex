                -->
                <h3 class="card-title plan-price">$15</h3>
                <p class="card-description">
                  5 <%= gettext "dedicated business phone number" %>.
                </p>
                <p class="card-description">
                  <%= gettext "Daily limit is" %> 1250 <%= gettext "messages" %>.
                </p>
                <%= hidden_input f, :plan_id, class: "plan", value: @gold_plan_id  %>
                <%= submit id: "Gold-Button", class: "btn btn-github btn-round choose-plan-button" do %>
                  <span class="fas fa-spinner fa-spin fa-lg hide-icon"></span>
